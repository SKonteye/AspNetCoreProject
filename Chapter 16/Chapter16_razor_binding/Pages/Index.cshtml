@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<h1>Square Calculation</h1>
<ul>
    @foreach (var value in new int[] { 1, 2, 3, 4, 5, 6, 7, 8, 9 })
    {
        <li>
            <a asp-page="calculateSquare" asp-route-number="@value"> Calculate the Square of @value </a>
        </li>
    }

</ul>

